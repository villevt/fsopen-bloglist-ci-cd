{
  "name": "bloglist",
  "version": "1.0.0",
  "description": "Bloglist application",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "start": "concurrently \"cross-env NODE_ENV=production node index.js\" \"react-scripts start\"",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "cypress:open": "concurrently \"cross-env NODE_ENV=test node index.js\" \"cypress open\"",
    "populate-db": "node utils/populate-db.js",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.14.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.0.0",
    "eslint": "^7.27.0",
    "jest": "^26.6.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.12",
    "mongoose-unique-validator": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "jest": {
    "projects": [
      {
        "testEnvironment": "node",
        "testMatch": [
          "<rootDir>/tests/*test.js"
        ]
      },
      {
        "testEnvironment": "jsdom",
        "testMatch": [
          "<rootDir>/frontend/**/**/*.test.js"
        ]
      }
    ]
  }
}
